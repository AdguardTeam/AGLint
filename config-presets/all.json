{
  "rules": {
    "if-directive-balance": "error",
    "max-css-selectors": [
      "warn",
      {
        "maxSelectors": 1
      }
    ],
    "no-css-comments": "warn",
    "no-duplicated-css-declaration-props": "error",
    "no-duplicated-hint-platforms": "error",
    "no-duplicated-hints": "error",
    "no-duplicated-modifiers": "error",
    "no-excluded-rules": [
      "error",
      {
        "excludedRuleTexts": [],
        "excludedRegExpPatterns": []
      }
    ],
    "no-inconsistent-hint-platforms": "error",
    "no-invalid-css-declaration": "error",
    "no-invalid-domains": "error",
    "no-invalid-hint-params": "error",
    "no-invalid-modifiers": "error",
    "no-short-rules": [
      "error",
      {
        "minLength": 4
      }
    ],
    "no-unknown-hint-platforms": "error",
    "no-unknown-hints": "error",
    "no-unknown-preprocessor-directives": "error",
    "no-unsupported-css-pseudo-class": [
      "error",
      {
        "fuzzyThreshold": 0.6
      }
    ],
    "scriptlet-quotes": [
      "error",
      {
        "adg": "double",
        "ubo": "none",
        "abp": "none",
        "disallowCurlyQuotes": true
      }
    ]
  }
}