{
    "name": "@adguard/aglint",
    "version": "1.0.11",
    "description": "Universal adblock filter list linter.",
    "keywords": [
        "adblock",
        "adguard",
        "filter",
        "linter",
        "list",
        "ublock"
    ],
    "author": "AdGuard Software Ltd.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AdguardTeam/AGLint.git"
    },
    "bugs": {
        "url": "https://github.com/AdguardTeam/AGLint/issues"
    },
    "homepage": "https://github.com/AdguardTeam/AGLint#readme",
    "type": "module",
    "main": "dist/aglint.cjs",
    "module": "dist/aglint.esm.js",
    "browser": "dist/aglint.umd.min.js",
    "types": "dist/aglint.d.ts",
    "bin": "dist/aglint.cli.js",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "build": "yarn clean && yarn build-types && yarn rollup --config rollup.config.ts --configPlugin @rollup/plugin-json --configPlugin @rollup/plugin-typescript && yarn clean-types",
        "build-types": "tsc --declaration --emitDeclarationOnly --outdir dist/types",
        "check-compatibility-tables": "node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node scripts/check-compatibility-tables.ts",
        "check-types": "tsc --noEmit",
        "clean": "rimraf dist",
        "clean-types": "rimraf dist/types",
        "coverage": "jest --coverage",
        "lint": "eslint . --ext .ts",
        "prepare": "husky install",
        "test": "jest --runInBand"
    },
    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@babel/preset-env": "^7.20.2",
        "@rollup/plugin-alias": "^4.0.2",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^23.0.2",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^9.0.2",
        "@swc/core": "^1.3.64",
        "@swc/jest": "^0.2.26",
        "@types/clone-deep": "^4.0.1",
        "@types/fs-extra": "^11.0.1",
        "@types/glob": "^8.1.0",
        "@types/globrex": "^0.1.2",
        "@types/jest": "^28.1.0",
        "@types/js-yaml": "^4.0.5",
        "@types/node": "^18.11.7",
        "@types/text-table": "^0.2.2",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.24.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-import-newlines": "^1.3.1",
        "eslint-plugin-jsdoc": "^39.6.2",
        "husky": "^8.0.3",
        "jest": "^29.1.2",
        "jsdoc": "^4.0.0",
        "rimraf": "^5.0.1",
        "rollup": "^3.2.3",
        "rollup-plugin-add-shebang": "^0.3.1",
        "rollup-plugin-dts": "^5.0.0",
        "rollup-plugin-node-externals": "^5.0.1",
        "rollup-plugin-polyfill-node": "^0.11.0",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "@adguard/agtree": "^1.0.1",
        "chalk": "4.1.2",
        "clone-deep": "^4.0.1",
        "commander": "^9.4.1",
        "deepmerge": "^4.2.2",
        "fast-deep-equal": "^3.1.3",
        "fs-extra": "^11.1.0",
        "glob": "^9.3.2",
        "ignore": "^5.2.1",
        "inflection": "^2.0.1",
        "js-yaml": "^4.1.0",
        "strip-ansi": "6.0.1",
        "superstruct": "^1.0.3",
        "terminal-link": "2.1.1",
        "text-table": "^0.2.0",
        "tldts": "^5.7.109",
        "xregexp": "^5.1.1"
    }
}
