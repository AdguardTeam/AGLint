{
    "compilerOptions": {
        "moduleResolution": "Bundler",
        "target": "ESNext",
        "module": "ESNext",
        "lib": ["esnext", "dom"],
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "allowJs": true,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
    }
}
